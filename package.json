{
  "name": "nestjs-prisma-swagger-app",
  "version": "1.0.0",
  "description": "A NestJS application integrated with Prisma and Swagger for API documentation.",
  "main": "dist/main.js",
  "scripts": {
    "start": "nest start",
    "build": "nest build",
    "build:all": "tsc --project tsconfig.build.json",
    "start:dev": "nest start --watch",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "test": "jest --runInBand --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:file": "jest --runInBand --detectOpenHandles --forceExit $FILE",
    "prisma": "prisma",
    "generate": "prisma generate",
    "seed": "ts-node src/database/prisma.seed.ts",
    "seed:local": "bash ./dev_scripts/prisma-local.sh npx prisma db seed",    
    "migrate:dev": "prisma migrate dev",
    "migrate:dev:local": "bash ./dev_scripts/prisma-local.sh npx prisma migrate:dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:deploy:local": "bash ./dev_scripts/prisma-local.sh npm run migrate:deploy",
    "migrate:status": "prisma migrate status",
    "migrate:status:local": "bash ./dev_scripts/prisma-local.sh npx prisma migrate status",
    "migrate:reset": "prisma migrate reset && npm run seed",
    "migrate:reset:local": "bash ./dev_scripts/prisma-local.sh npm run migrate:reset",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"*.{js,ts,json,md}\"",
    "migrate:local": "bash ./dev_scripts/prisma-local.sh npx prisma migrate deploy",
    "studio:local": "???"
  },
  "dependencies": {
    "@automapper/classes": "^8.8.1",
    "@automapper/core": "^8.8.1",
    "@automapper/nestjs": "^8.8.1",
    "@automapper/types": "^6.3.1",
    "@azure/msal-node": "^3.6.0",
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.9.0",
    "@types/multer": "^1.4.13",
    "class-transformer": "^0.5.1",
    "dotenv": "^16.5.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "multer": "^2.0.1",
    "nestjs-pino": "^4.4.0",
    "pg": "^8.16.0",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "prisma": "^6.9.0",
    "redis": "^5.5.5",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.7",
    "@nestjs/testing": "^11.1.3",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.29",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "license": "MIT",
  "private": true
}
